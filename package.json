{
  "name": "unguess-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "yarn test -- --coverage",
    "test": "tsnd -T node_modules/jest/bin/jest.js ",
    "test:ls": "yarn test -- --json",
    "test:watch": "tsnd -T node_modules/jest/bin/jest.js --watch",
    "clean": "rimraf dist",
    "transpile": "tsc --noEmit false && tsc-alias",
    "copy-reference": "copyfiles -f src/reference/* dist/reference/",
    "build": "npm-run-all clean transpile copy-reference",
    "start": "tsnd --respawn -r tsconfig-paths/register  --transpile-only  ./src/index.ts",
    "postinstall": "husky install",
    "rename-schema": "node -e \"require('fs').rename('postman/schemas/openapi.yml','postman/schemas/schema.yaml', (err) => { (err ? console.log(err) : console.log('Rename complete!')); } ) \"",
    "update-postman": "copyfiles -f src/reference/openapi.yml postman/schemas && npm run rename-schema",
    "types": "openapi-typescript src/reference/openapi.yml --output src/schema.ts "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@appquality/wp-auth": "^1.0.7",
    "@babel/runtime": "^7.16.7",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^0.26.1",
    "body-parser": "^1.19.1",
    "comment-parser": "^1.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "glob": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "openapi-backend": "^3.9.2",
    "php-unserialize": "^0.0.1",
    "wordpress-hash-node": "^1.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.196",
    "@swc/jest": "^0.2.21",
    "@types/better-sqlite3": "^7.6.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.1.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mysql": "^2.15.20",
    "@types/supertest": "^2.0.11",
    "@types/wordpress-hash-node": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "better-sqlite3": "^8.0.1",
    "commitizen": "^4.2.5",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "jest": "^28.1.0",
    "jest-cli": "^28.1.0",
    "jest-expect-message": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^5.1.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
